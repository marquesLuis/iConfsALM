<h1>Listing maps_versions</h1>

<table>
  <tr>
    <th>Version</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @maps_versions.each do |maps_version| %>
  <tr>
    <td><%= maps_version.version %></td>
    <td><%= link_to 'Show', maps_version %></td>
    <td><%= link_to 'Edit', edit_maps_version_path(maps_version) %></td>
    <td><%= link_to 'Destroy', maps_version, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Maps version', new_maps_version_path %>
