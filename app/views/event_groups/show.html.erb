<p id="notice"><%= notice %></p>

<p>
  <b>Date:</b>
  <%= @event_group.date %>
</p>

<p>
  <b>End:</b>
  <%= @event_group.end %>
</p>

<p>
  <b>Location:</b>
  <%= @event_group.location_id %>
</p>

<p>
  <b>Areas:</b>
</p>
