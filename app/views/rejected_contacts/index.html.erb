<h1>Listing rejected_contacts</h1>

<table>
  <tr>
    <th>Requester</th>
    <th>Requested</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @rejected_contacts.each do |rejected_contact| %>
  <tr>
    <td><%= rejected_contact.requester_id %></td>
    <td><%= rejected_contact.requested_id %></td>
    <td><%= link_to 'Show', rejected_contact %></td>
    <td><%= link_to 'Edit', edit_rejected_contact_path(rejected_contact) %></td>
    <td><%= link_to 'Destroy', rejected_contact, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

