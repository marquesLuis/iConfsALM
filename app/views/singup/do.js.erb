
$('#user_data').append("<p><%= @person.prefix %></p> ")
$('#user_data').append("<p><%= @person.first_name %></p> ")
$('#user_data').append("<p><%= @person.last_name %></p> ")
$('#user_data').append("<p><%= @person.affiliation %></p> ")
$('#user_data').append("<p><%= @person.email %></p> ")
$('#user_data').append("<p><%= @person.signup_code %></p> ")

$('#user_data').append("<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %> <%= devise_error_messages! %> <p>Hi</p> <div><%= f.label :email %><br /> <%= f.email_field :email, :autofocus => true %></div> <div><%= f.label :password %><br />  <%= f.password_field :password %></div> <div><%= f.label :password_confirmation %><br /> <%= f.password_field :password_confirmation %></div> <div><%= f.submit "Sign up" %></div> <% end %>");