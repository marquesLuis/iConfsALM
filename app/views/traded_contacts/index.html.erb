<h1>Listing traded_contacts</h1>

<table>
  <tr>
    <th>Requester</th>
    <th>Requested</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @traded_contacts.each do |traded_contact| %>
  <tr>
    <td><%= traded_contact.requester_id %></td>
    <td><%= traded_contact.requested_id %></td>
    <td><%= link_to 'Show', traded_contact %></td>
    <td><%= link_to 'Edit', edit_traded_contact_path(traded_contact) %></td>
    <td><%= link_to 'Destroy', traded_contact, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

